import java.util.*;

class Node {
    Node prev;
    Node next;
    int data;

    Node(int data) {
        this.data = data;
        prev = null;
        next = null;
    }
}

class DLink {
    Node head = null;

    void atFirst(int data) {
        Node newnode = new Node(data);
        if (head == null) {
            head = newnode;
        } else {
            newnode.next = head;
            head.prev = newnode;
            head = newnode;
        }
    }

    void atLast(int data) {
        Node newnode = new Node(data);
        if (head == null) {
            head = newnode;
        } else {
            Node temp = head;
            while (temp.next != null) {
                temp = temp.next;
            }
            temp.next = newnode;
            newnode.prev = temp;
        }
    }

    void display() {
        Node temp = head;
        while (temp != null) {
            System.out.print(temp.data + " ");
            temp = temp.next;
        }
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        DLink list = new DLink();

        list.atFirst(10);
        list.atFirst(20);
        list.atLast(5);
        list.atLast(1);

        System.out.print("Doubly Linked List: ");
        list.display();
    }
}
