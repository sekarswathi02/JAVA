import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int[][] num = new int[2][3];
        System.out.println("Enter numbers array:");
        for (int i = 0; i < 2; i++) {
            for (int j = 0; j < 3; j++) {
                num[i][j] = sc.nextInt();
            }
        }
        System.out.println("The array you entered is:");
        System.out.println(Arrays.deepToString(num));
    }
}
